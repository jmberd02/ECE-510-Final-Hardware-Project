#include "xparameters.h"
#include "xbasic_types.h"
#include "xgpio.h"
#include "xstatus.h"



XGpio GpioDevice;

int main(void)
{
	//Variables


	//GPIO init
	status = XGpio_Initialize(&GpioDevice,XPAR_GPIO_DEVICE_ID);
	if (status != XST_SUCCESS)
		return XST_FAILURE;

	XGpio_SetDataDirection(&GpioDevice,1,0x0000000000);
	XGpio_SetDataDirection(&GpioDevice,2,0xFFFFFFFFFF);

	for(;;)
	{

	}
}

